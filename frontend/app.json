{
  "expo": {
    "name": "Neighbr HOA Assistant",
    "slug": "neighbr-hoa-assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "neighbr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.neighbr.app"
        }
      ],
      "expo-document-picker",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
